Gramática

    0 $accept: instruccion $end

    1 instruccion: instruccion_expresion
    2            | instruccion_bifurcacion
    3            | instruccion_bucle
    4            | instruccion_salto
    5            | instruccion_destino_salto
    6            | instruccion_devolver
    7            | instruccion_vacia
    8            | instruccion_lanzamiento_excepcion
    9            | instruccion_captura_excepcion
   10            | error

   11 instruccion_expresion: expresion_funcional ';'
   12                      | asignacion ';'

   13 asignacion: expresion_indexada operador_asignacion expresion

   14 operador_asignacion: '='
   15                    | SUMA_ASIG
   16                    | RESTA_ASIG
   17                    | MULT_ASIG
   18                    | DIV_ASIG
   19                    | MOD_ASIG
   20                    | POT_ASIG
   21                    | FI_ASIG
   22                    | FD_ASIG
   23                    | AND_ASIG
   24                    | XOR_ASIG
   25                    | OR_ASIG

   26 instruccion_bifurcacion: SI '(' expresion ')' accion rep_otros_casos op_sino_accion FIN

   27 op_sino_accion: SINO accion
   28               | %empty

   29 rep_otros_casos: rep_otros_casos otros_casos
   30                | %empty

   31 otros_casos: SI ENCAMBIO '(' expresion ')' accion

   32 accion: instruccion
   33       | bloque_instrucciones

   34 instruccion_bucle: MIENTRAS '(' expresion ')' accion
   35                  | HACER accion MIENTRAS '(' expresion ')' ';'
   36                  | PARA '(' l_asignaciones ';' expresion ';' l_asignaciones ')' accion
   37                  | PARA CADA IDENTIFICADOR '(' expresion ')' accion

   38 l_asignaciones: l_asignaciones ',' asignacion
   39               | asignacion

   40 instruccion_salto: SALTAR IDENTIFICADOR ';'
   41                  | CONTINUAR ';'
   42                  | ESCAPE ';'

   43 instruccion_destino_salto: ETIQUETA IDENTIFICADOR ';'

   44 instruccion_devolver: DEVOLVER ';'
   45                     | DEVOLVER expresion ';'

   46 instruccion_vacia: ';'

   47 instruccion_lanzamiento_excepcion: LANZA EXCEPCION IDENTIFICADOR ';'

   48 instruccion_captura_excepcion: EJECUTA bloque_instrucciones clausulas

   49 clausulas: clausulas_excepcion op_clausula_defecto
   50          | clausula_defecto

   51 op_clausula_defecto: clausula_defecto
   52                    | %empty

   53 clausulas_excepcion: rep_clausula_excepcion_especifica clausula_excepcion_general

   54 rep_clausula_excepcion_especifica: rep_clausula_excepcion_especifica clausula_excepcion_especifica
   55                                  | %empty

   56 clausula_excepcion_especifica: EXCEPCION IDENTIFICADOR bloque_instrucciones

   57 clausula_excepcion_general: OTRA EXCEPCION bloque_instrucciones

   58 clausula_defecto: DEFECTO bloque_instrucciones

   59 bloque_instrucciones: GE

   60 agrup_expresion: expresion_constante
   61                | expresion_indexada
   62                | expresion_funcional

   63 expresion_constante: CTC_ENTERA
   64                    | CTC_REAL
   65                    | CTC_CADENA
   66                    | CTC_CARACTER

   67 expresion_indexada: expresion_basica
   68                   | expresion_indexada '.' expresion_basica
   69                   | expresion_indexada INDIRECCION expresion_basica
   70                   | expresion_indexada op_indireccion indice

   71 op_indireccion: INDIRECCION
   72               | %empty

   73 expresion_basica: IDENTIFICADOR
   74                 | '(' expresion ')'
   75                 | '^' expresion_basica
   76                 | '\\' expresion_basica

   77 indice: '[' expresion ']'
   78       | '{' expresion '}'

   79 expresion_funcional: IDENTIFICADOR '(' l_expresion ')'

   80 l_expresion: l_expresiones
   81            | %empty

   82 l_expresiones: l_expresiones ',' expresion
   83              | expresion

   84 expresion: expresion_logica op_exp

   85 op_exp: SI expresion SINO expresion
   86       | %empty

   87 expresion_logica: expresion_logica_or

   88 expresion_logica_or: expresion_logica_or OR expresion_logica_and
   89                    | expresion_logica_and

   90 expresion_logica_and: expresion_logica_and AND expresion_eq
   91                     | expresion_eq

   92 expresion_eq: expresion_eq EQ expresion_comp
   93             | expresion_eq NEQ expresion_comp
   94             | expresion_comp

   95 expresion_comp: expresion_comp '<' expresion_bin_or
   96               | expresion_comp '>' expresion_bin_or
   97               | expresion_comp LE expresion_bin_or
   98               | expresion_comp GE expresion_bin_or
   99               | expresion_bin_or

  100 expresion_bin_or: expresion_bin_or '|' expresion_bin_xor
  101                 | expresion_bin_xor

  102 expresion_bin_xor: expresion_bin_xor '@' expresion_bin_and
  103                  | expresion_bin_and

  104 expresion_bin_and: expresion_bin_and '&' expresion_desp
  105                  | expresion_desp

  106 expresion_desp: expresion_desp FLECHA_DCHA expresion_aditiva
  107               | expresion_desp FLECHA_IZDA expresion_aditiva
  108               | expresion_aditiva

  109 expresion_aditiva: expresion_aditiva '+' expresion_multiplicativa
  110                  | expresion_aditiva '-' expresion_multiplicativa
  111                  | expresion_multiplicativa

  112 expresion_multiplicativa: expresion_multiplicativa '*' expresion_potencia
  113                         | expresion_multiplicativa '/' expresion_potencia
  114                         | expresion_multiplicativa '%' expresion_potencia
  115                         | expresion_potencia

  116 expresion_potencia: expresion_noassoc POTENCIA expresion_potencia
  117                   | expresion_noassoc

  118 expresion_noassoc: '-' agrup_expresion
  119                  | '~' agrup_expresion
  120                  | '!' agrup_expresion
  121                  | TAMANO agrup_expresion
  122                  | agrup_expresion


