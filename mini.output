    0 $accept: programa $end

    1 programa: inicio_programa bloque_programa

    2 inicio_programa: PROGRAMA IDENTIFICADOR ';' l_cabecera

    3 l_cabecera: l_cabecera CABECERA l_rutas ';'
    4           | %empty

    5 l_rutas: l_rutas ',' RUTA
    6        | RUTA

    7 $@1: %empty

    8 $@2: %empty

    9 $@3: %empty

   10 $@4: %empty

   11 bloque_programa: declaraciones_tipos $@1 declaraciones_constantes $@2 declaraciones_variables $@3 declaraciones_funciones $@4 bloque_instrucciones
   12                | error ';'

   13 declaraciones_tipos: TIPOS l_decl_tipo FIN
   14                    | %empty
   15                    | error ';'

   16 l_decl_tipo: l_decl_tipo declaracion_tipo
   17            | declaracion_tipo

   18 declaracion_tipo: IDENTIFICADOR ES especificacion_tipo ';'

   19 especificacion_tipo: l_ref tipo_basico

   20 l_ref: l_ref REF
   21      | %empty

   22 tipo_basico: IDENTIFICADOR
   23            | tipo_escalar
   24            | tipo_enumerado
   25            | tipo_estructurado

   26 tipo_escalar: ENTERO
   27             | REAL
   28             | CARACTER
   29             | CADENA
   30             | FICHERO
   31             | EXCEPCION

   32 tipo_enumerado: ARRAY DE especificacion_tipo
   33               | HASH DE especificacion_tipo
   34               | CONJUNTO DE especificacion_tipo

   35 tipo_estructurado: ESTRUCTURA PRINCIPIO lista_campo FIN
   36                  | UNION PRINCIPIO lista_campo FIN

   37 lista_campo: lista_campo linea_campo
   38            | linea_campo

   39 linea_campo: rep_ident ES especificacion_tipo ';'

   40 rep_ident: rep_ident ',' IDENTIFICADOR
   41          | IDENTIFICADOR

   42 declaraciones_constantes: CONSTANTES l_declaraciones_constantes FIN
   43                         | %empty

   44 l_declaraciones_constantes: l_declaraciones_constantes declaracion_constante
   45                           | declaracion_constante

   46 declaracion_constante: IDENTIFICADOR ES tipo_basico '=' constante ';'

   47 constante: CTC_ENTERA
   48          | CTC_REAL
   49          | CTC_CARACTER
   50          | CTC_CADENA
   51          | constante_enumerada
   52          | constante_estructurada

   53 constante_enumerada: '(' l_constante ')'
   54                    | '(' l_elemento_hash ')'

   55 l_constante: rep_constante
   56            | %empty

   57 rep_constante: rep_constante ',' constante
   58              | constante

   59 l_elemento_hash: rep_elemento_hash

   60 rep_elemento_hash: rep_elemento_hash ',' elemento_hash
   61                  | elemento_hash

   62 elemento_hash: CTC_CADENA FD_ASIG constante

   63 constante_estructurada: '{' l_campo_constante '}'

   64 l_campo_constante: l_campo_constante ',' campo_constante
   65                  | campo_constante

   66 campo_constante: IDENTIFICADOR '=' constante

   67 declaraciones_variables: VARIABLES l_declaracion FIN
   68                        | %empty

   69 l_declaracion: l_declaracion declaracion_variables
   70              | declaracion_variables

   71 declaracion_variables: rep_ident ':' especificacion_tipo ';'
   72                      | rep_ident ':' especificacion_tipo '=' l_expr ';'

   73 declaraciones_funciones: declaraciones_funciones declaracion_funcion
   74                        | %empty

   75 declaracion_funcion: FUNCION IDENTIFICADOR op_lista_parametros FLECHA_DCHA tipo_salida cuerpo_funcion

   76 op_lista_parametros: lista_parametros
   77                    | %empty

   78 lista_parametros: '(' rep_parametros parametros ')'

   79 rep_parametros: rep_parametros parametros ';'
   80               | %empty

   81 parametros: rep_ident ':' especificacion_tipo
   82           | rep_ident ':' especificacion_tipo '=' rep_expr_constante

   83 rep_expr_constante: rep_expr_constante ',' expresion_constante
   84                   | expresion_constante

   85 tipo_salida: especificacion_tipo
   86            | NADA

   87 $@5: %empty

   88 $@6: %empty

   89 $@7: %empty

   90 cuerpo_funcion: declaraciones_constantes $@5 declaraciones_variables $@6 declaraciones_funciones $@7 bloque_instrucciones
   91               | error ';'

   92 bloque_instrucciones: PRINCIPIO l_instruccion FIN

   93 l_instruccion: l_instruccion instruccion
   94              | instruccion

   95 instruccion: instruccion_expresion
   96            | instruccion_bifurcacion
   97            | instruccion_bucle
   98            | instruccion_salto
   99            | instruccion_destino_salto
  100            | instruccion_devolver
  101            | instruccion_vacia
  102            | instruccion_lanzamiento_excepcion
  103            | instruccion_captura_excepcion
  104            | error ';'

  105 instruccion_expresion: expresion_funcional ';'
  106                      | asignacion ';'

  107 asignacion: expresion_indexada operador_asignacion expresion

  108 operador_asignacion: '='
  109                    | SUMA_ASIG
  110                    | RESTA_ASIG
  111                    | MULT_ASIG
  112                    | DIV_ASIG
  113                    | MOD_ASIG
  114                    | POT_ASIG
  115                    | FI_ASIG
  116                    | FD_ASIG
  117                    | AND_ASIG
  118                    | XOR_ASIG
  119                    | OR_ASIG

  120 instruccion_bifurcacion: SI '(' expresion ')' accion rep_otros_casos op_sino_accion FIN

  121 op_sino_accion: SINO accion
  122               | %empty

  123 rep_otros_casos: rep_otros_casos otros_casos
  124                | %empty

  125 otros_casos: SI ENCAMBIO '(' expresion ')' accion

  126 accion: instruccion
  127       | bloque_instrucciones

  128 instruccion_bucle: MIENTRAS '(' expresion ')' accion
  129                  | HACER accion MIENTRAS '(' expresion ')' ';'
  130                  | PARA '(' l_asignaciones ';' expresion ';' l_asignaciones ')' accion
  131                  | PARA CADA IDENTIFICADOR '(' expresion ')' accion

  132 l_asignaciones: l_asignaciones ',' asignacion
  133               | asignacion

  134 instruccion_salto: SALTAR IDENTIFICADOR ';'
  135                  | CONTINUAR ';'
  136                  | ESCAPE ';'

  137 instruccion_destino_salto: ETIQUETA IDENTIFICADOR ';'

  138 instruccion_devolver: DEVOLVER ';'
  139                     | DEVOLVER expresion ';'

  140 instruccion_vacia: ';'

  141 instruccion_lanzamiento_excepcion: LANZA EXCEPCION IDENTIFICADOR ';'

  142 instruccion_captura_excepcion: EJECUTA bloque_instrucciones clausulas

  143 clausulas: clausulas_excepcion op_clausula_defecto
  144          | clausula_defecto

  145 op_clausula_defecto: clausula_defecto
  146                    | %empty

  147 clausulas_excepcion: rep_clausula_excepcion_especifica clausula_excepcion_general

  148 rep_clausula_excepcion_especifica: rep_clausula_excepcion_especifica clausula_excepcion_especifica
  149                                  | %empty

  150 clausula_excepcion_especifica: EXCEPCION IDENTIFICADOR bloque_instrucciones

  151 clausula_excepcion_general: OTRA EXCEPCION bloque_instrucciones

  152 clausula_defecto: DEFECTO bloque_instrucciones

  153 agrup_expresion: expresion_constante
  154                | expresion_indexada
  155                | expresion_funcional
  156                | error ';'

  157 expresion_constante: CTC_ENTERA
  158                    | CTC_REAL
  159                    | CTC_CADENA
  160                    | CTC_CARACTER

  161 expresion_indexada: expresion_basica
  162                   | expresion_indexada '.' expresion_basica
  163                   | expresion_indexada INDIRECCION expresion_basica
  164                   | expresion_indexada op_indireccion indice

  165 op_indireccion: INDIRECCION
  166               | %empty

  167 expresion_basica: IDENTIFICADOR
  168                 | '(' expresion ')'
  169                 | '^' expresion_basica
  170                 | '\\' expresion_basica

  171 indice: '[' expresion ']'
  172       | '{' expresion '}'

  173 expresion_funcional: IDENTIFICADOR '(' l_expresion ')'

  174 l_expresion: l_expr
  175            | %empty

  176 l_expr: l_expr ',' expresion
  177       | expresion

  178 expresion: expresion_logica op_exp

  179 op_exp: SI expresion SINO expresion
  180       | %empty

  181 expresion_logica: expresion_logica_or

  182 expresion_logica_or: expresion_logica_or OR expresion_logica_and
  183                    | expresion_logica_and

  184 expresion_logica_and: expresion_logica_and AND expresion_eq
  185                     | expresion_eq

  186 expresion_eq: expresion_eq EQ expresion_comp
  187             | expresion_eq NEQ expresion_comp
  188             | expresion_comp

  189 expresion_comp: expresion_comp '<' expresion_bin_or
  190               | expresion_comp '>' expresion_bin_or
  191               | expresion_comp LE expresion_bin_or
  192               | expresion_comp GE expresion_bin_or
  193               | expresion_bin_or

  194 expresion_bin_or: expresion_bin_or '|' expresion_bin_xor
  195                 | expresion_bin_xor

  196 expresion_bin_xor: expresion_bin_xor '@' expresion_bin_and
  197                  | expresion_bin_and

  198 expresion_bin_and: expresion_bin_and '&' expresion_desp
  199                  | expresion_desp

  200 expresion_desp: expresion_desp FLECHA_DCHA expresion_aditiva
  201               | expresion_desp FLECHA_IZDA expresion_aditiva
  202               | expresion_aditiva

  203 expresion_aditiva: expresion_aditiva '+' expresion_multiplicativa
  204                  | expresion_aditiva '-' expresion_multiplicativa
  205                  | expresion_multiplicativa

  206 expresion_multiplicativa: expresion_multiplicativa '*' expresion_potencia
  207                         | expresion_multiplicativa '/' expresion_potencia
  208                         | expresion_multiplicativa '%' expresion_potencia
  209                         | expresion_potencia

  210 expresion_potencia: expresion_noassoc POTENCIA expresion_potencia
  211                   | expresion_noassoc

  212 expresion_noassoc: '-' agrup_expresion
  213                  | '~' agrup_expresion
  214                  | '!' agrup_expresion
  215                  | TAMANO agrup_expresion
  216                  | agrup_expresion


